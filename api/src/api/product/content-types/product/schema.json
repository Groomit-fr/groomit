{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 25
    },
    "description": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "isAvailaible": {
      "type": "boolean",
      "default": true
    },
    "price": {
      "type": "float",
      "required": true
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "line1": {
      "type": "string",
      "default": "Imprim√© en France",
      "required": true,
      "maxLength": 20
    },
    "line2": {
      "type": "string",
      "default": "Coton Bio - EcoTech",
      "required": true,
      "maxLength": 20
    },
    "line3": {
      "type": "string",
      "required": true,
      "default": "Coupe Unisex",
      "maxLength": 20
    },
    "line4": {
      "type": "string"
    }
  }
}
